{
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=web",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish-pkg": "cp README.md packages/rsc-daisyui && cd packages/rsc-daisyui && pnpm publish-pkg && rm README.md",
    "build-storybook": "cd packages/rsc-daisyui && pnpm build-storybook"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tsconfig": "workspace:*",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@9.2.0",
  "name": "rsc-daisyui"
}
