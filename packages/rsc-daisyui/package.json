{
  "name": "rsc-daisyui",
  "main": "./dist/index.js",
  "version": "0.6.0",
  "description": "daisyUI for React Server Component",
  "homepage": "https://yoshi6jp.github.io/rsc-daisyui/",
  "bugs": {
    "url": "https://github.com/yoshi6jp/rsc-daisyui/issues"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.mjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoshi6jp/rsc-daisyui.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "publish-packages": "turbo run build lint && changeset version && changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-storysource": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@tailwindcss/typography": "^0.5.10",
    "@tw-classed/core": "^1.7.0",
    "@types/react": "^18.2.48",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-storybook": "^0.6.15",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.10",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.4.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@tw-classed/react": "^1.7.0",
    "tailwind-merge": "^2.2.0"
  }
}
