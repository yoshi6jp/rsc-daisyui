{
  "name": "rsc-daisyui",
  "main": "./dist/index.js",
  "version": "1.0.3",
  "description": "daisyUI for React Server Component",
  "homepage": "https://yoshi6jp.github.io/rsc-daisyui/",
  "keywords": [
    "react",
    "daisyui",
    "component",
    "rsc"
  ],
  "bugs": {
    "url": "https://github.com/yoshi6jp/rsc-daisyui/issues"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.mjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yoshi6jp/rsc-daisyui.git"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "publish-pkg": "turbo run build lint && changeset version && changeset publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@chromatic-com/storybook": "^1",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-storysource": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@tailwindcss/typography": "^0.5.14",
    "@tw-classed/core": "^1.7.0",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-github-btn": "^1.4.0",
    "storybook": "^8.2.9",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.4.10",
    "tsconfig": "workspace:*",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@tw-classed/react": "^1.7.0",
    "tailwind-merge": "^2.5.2"
  }
}
